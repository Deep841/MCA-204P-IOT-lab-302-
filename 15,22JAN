####Answer-1: 
###1.1
from math import factorial

def Sum(x, n):
    sum = 1
    for i in range(1, n + 1):
        sum += (x ** i) / factorial(i)
    return sum

x = float(input("Enter the value of x: "))
n = int(input("Enter the value of n: "))

result = Sum(x, n)
print(f"The sum of the series is: {result}")

###1.2
def fact(num):
    if num==0 :
        return 1
    else:
        return num * fact(num-1)

def Sum(x,n):
    sum = 1
    for i in range(1,n+1):
        sum += ( x**i ) / fact(i)
    return sum 

x = float(input("x : "))
n = int(input("n : "))

result = Sum(x,n)
print(result)



####Answer-2:
import random
r_nums = []

for _ in range(100): # _ acts as an place holder. 
    r_nums.append(random.randint(100, 900))

print(r_nums)
print("no. of elts : ",len(r_nums))

#part1 : 
print(" ")
print("part(i)... ")
count = 0
for i in r_nums :
    if i%2 != 0:
        count += 1
        print(i,end = " ")
print(" ")
print("no. of elts : ",count)

#part2 : 
print(" ")
print("part(ii)...")
count = 0
for i in r_nums :
    if i%2 == 0:
        count += 1
        print(i,end = " ")
print(" ")
print("no. of elts : ",count)

