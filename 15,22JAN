def is_prime(num):
    if num < 2:
        return False
    for i in range(2, int(num ** 0.5) + 1):
        if num % i == 0:
            return False
    return True

for r in r_nums:
    if is_prime(r):
        count += 1
        print(r,end=" ")
print(" ")
print("no. of prime numbers: ", count)



####Answer-3:
import math 
def is_prime(n):
    if n < 2:
        return False
    for i in range(2, int(math.sqrt(n))+1):
        if n%i == 0:
            return False
    return True

a = int(input("enter first number : "))
b = int(input("enter second number : "))

for i in range(a,b+1):
    if is_prime(i):
        print(i,end=" ")
    else:
        continue
print(" ")



